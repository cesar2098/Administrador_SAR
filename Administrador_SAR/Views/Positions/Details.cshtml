@model Administrador_SAR.DBContext.Position

@{
    ViewBag.Title = "Details";
    ViewBag.SubTitle = "Cargos Información";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card-box mb-30 p-2">
    <div>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Descripción
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>

            <dt>
                Activo
            </dt>

            <dd>
                @Html.DisplayFor(model => model.IsActive)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Editar", "Edit", new { id = Model.Id }, new { @class = "btn btn-warning" }) |
        @Html.ActionLink("Volver", "Index", null, new { @class = "btn btn-dark" })
    </p>
</div>
